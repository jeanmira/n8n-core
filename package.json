{
  "name": "n8n-private",
  "version": "1.0.0",
  "description": "Private n8n repository with custom nodes and workflows",
  "main": "index.js",
  "scripts": {
    "setup": "./setup.sh",
    "start": "./start.sh",
    "stop": "./stop.sh",
    "start:prod": "docker-compose -f infrastructure/docker-compose.prod.yml up -d",
    "start:test": "docker-compose -f infrastructure/docker-compose.test.yml up -d",
    "logs": "docker-compose logs -f",
    "restart": "docker-compose restart",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "build": "cd custom-nodes && npm run build",
    "build:watch": "cd custom-nodes && npm run build:watch",
    "export": "./scripts/export-workflows.sh",
    "import": "./scripts/import-workflows.sh",
    "validate": "./scripts/validate-workflows.sh",
    "deploy": "./scripts/deploy.sh",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:prod": "./scripts/deploy.sh production"
  },
  "keywords": [
    "n8n",
    "automation",
    "workflow",
    "no-code"
  ],
  "author": "Jean Mira <jeandemira@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.3.0",
    "eslint": "^8.45.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "n8n-workflow": "^1.0.0"
  }
}
